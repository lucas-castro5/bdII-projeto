-- Criação das tabelas
create table usuario(
	id_usuario INT PRIMARY KEY,
	nome VARCHAR(100),
	cpf VARCHAR(11),
	email VARCHAR(50),
	senha VARCHAR(200),
	telefone VARCHAR(15),
	tipo_usuario VARCHAR(20) CHECK (tipo_usuario IN('aluno','professor','coordenador','responsavel','administrador')),
	data_cadastro DATE
);
